#!/usr/bin/env ruby
#^syntax detection

forge "https://forgeapi.puppetlabs.com"


## OpenStack modules

mod 'puppet-neutron',
  :git => 'https://github.com/openstack/puppet-neutron',
  :ref => 'stable/mitaka'

## External modules

mod 'puppetlabs/inifile'
mod 'puppetlabs/apt'
mod 'puppetlabs/java'
mod 'puppetlabs/tomcat'
mod 'puppetlabs/stdlib'
mod 'puppetlabs/apache'
mod 'puppetlabs/concat'
